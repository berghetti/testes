cvlc -vvv /home/mayco/VÃ­deos/zabbix.mp4 --sout '#transcode{vcodec=h264,acodec=mpga,ab=128,channels=2,samplerate=44100,scodec=none}:rtp{dst=224.0.0.61,port=5004,mux=ts}' :no-sout-all :sout-keep

